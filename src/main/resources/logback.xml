<?xml version="1.0" encoding="UTF-8"?>
<configuration>

	<appender name="default" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} %-5level [%thread] %logger{26}:%line - %msg%n</pattern>
        </encoder>
    </appender>
	
	<root level="DEBUG">
		<appender-ref ref="default" />
	</root>
	
</configuration>